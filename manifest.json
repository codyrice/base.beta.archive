{
  "name": "Base Beta Archive Extension",
  "description": "Base Level Extension",
  "version": "1.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/main.js"],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "background": {
    "service_worker": "scripts/background.js"
  }
}
